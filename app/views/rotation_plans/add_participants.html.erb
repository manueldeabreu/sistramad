<div class="container"> 
  <div class="panel panel-primary">
    <div class="panel-heading">Busqueda de usuarios: </div>
      <div class="panel-body">
        <div class="row col-md-12">
          <%= render 'lookup'%>
          <br>
          <br>
          <br>
          <br>
          <% if @participants.size > 0 %>
            <%= render 'participants'%>
          <% end %>
        </div>

        <div class="text-center">
          <%= link_to rotation_plan_path(@procedure), class: 'btn btn-info btn-md list' do %>
            Regresar al plan
          <% end %>
        </div>
      </div>
    </div>
  </div>  
</div>



